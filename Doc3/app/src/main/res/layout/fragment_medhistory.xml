<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="316dp"
        android:layout_height="47dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/txt_prev_med" />

    <HorizontalScrollView
        android:layout_width="382dp"
        android:layout_height="618dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.969">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/med_hist_1"
                android:layout_width="430dp"
                android:layout_height="532dp"
                android:background="@null"
                android:scaleType="fitCenter"
                app:srcCompat="@drawable/med_hist_1" />

            <ImageView
                android:id="@+id/med_hist_2"
                android:layout_width="430dp"
                android:layout_height="532dp"
                android:background="@null"
                app:srcCompat="@drawable/med_hist_2" />

            <ImageView
                android:id="@+id/med_hist_3"
                android:layout_width="430dp"
                android:layout_height="532dp"
                android:background="@null"
                app:srcCompat="@drawable/med_hist_3" />

            <ImageView
                android:id="@+id/med_hist_4"
                android:layout_width="430dp"
                android:layout_height="532dp"
                android:background="@null"
                app:srcCompat="@drawable/med_hist_4" />

            <ImageView
                android:id="@+id/med_hist_5"
                android:layout_width="430dp"
                android:layout_height="532dp"
                android:background="@null"
                app:srcCompat="@drawable/med_hist_5" />

            <ImageView
                android:id="@+id/med_hist_6"
                android:layout_width="430dp"
                android:layout_height="532dp"
                android:background="@null"
                app:srcCompat="@drawable/med_hist_6" />
        </LinearLayout>
    </HorizontalScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>